//REVISADO
model Reporte {
  id                    Int           @id @default(autoincrement())
  nombre                String
  descripcion           String
  consultaSql           String
  esStoredProcedure     Boolean
  activo                Boolean
  fechaGeneracionArchivo DateTime?
  estadoReporteId       Int
  encuestaId            Int?

  estadoReporte         EstadoReporte  @relation(fields: [estadoReporteId], references: [id])
  encuesta              Encuesta?      @relation(fields: [encuestaId], references: [id])

  @@map("Reporte")
}