//REVISADO
model SubSectorEconomico {
  id               Int              @id @default(autoincrement())
  nombre           String
  activo           Boolean
  sectorEconomicoId Int

  sectorEconomico  SectorEconomico  @relation(fields: [sectorEconomicoId], references: [id])
  empresas         Empresa[]
  transaccionesDest Transaccion[]

  @@map("SubSectorEconomico")
}