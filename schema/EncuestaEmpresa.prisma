//REVISADO
model EncuestaEmpresa {
  empresaId        String
  encuestaId       Int
  estadoEmpresaId  Int

  empresa          Empresa        @relation(fields: [empresaId], references: [id])
  encuesta         Encuesta       @relation(fields: [encuestaId], references: [id])
  estadoEmpresa    EstadoEmpresa  @relation(fields: [estadoEmpresaId], references: [id])

  @@id([empresaId, encuestaId])
  @@map("EncuestaEmpresa")
}