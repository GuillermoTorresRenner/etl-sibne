//REVISADO - Estructura basada en AspNetUserTokens
model UsuarioToken {
  userId                String
  loginProvider         String
  name                  String
  value                 String?
  
  // Relaci√≥n con Usuario
  usuario               Usuario @relation(fields: [userId], references: [id], onDelete: Cascade)
  
  @@id([userId, loginProvider, name])
}