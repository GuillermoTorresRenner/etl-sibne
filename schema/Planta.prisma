//REVISADO
model Planta {
  id            Int            @id @default(autoincrement())
  codigo        Int
  nombre        String
  activo        Boolean
  comunaId      Int
  empresaId     String

  comuna        Comuna         @relation(fields: [comunaId], references: [id])
  empresa       Empresa        @relation(fields: [empresaId], references: [id])
  encuestaPlanta EncuestaPlanta[]
  transacciones  Transaccion[]
}