//REVISADO
model CargaMasivaError {
  id                   Int                @id @default(autoincrement())
  cargaMasivaArchivoId Int
  linea                Int
  columna              Int
  nombreCampo          String
  descripcion          String
  cargaMasivaDetalleId Int
  empresaId            String

  cargaMasivaDetalle   CargaMasivaDetalle @relation(fields: [cargaMasivaDetalleId], references: [id])

  @@map("CargaMasivaError")
}