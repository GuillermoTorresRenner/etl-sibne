//REVISADO
model Contacto {
  id                Int    @id @default(autoincrement())
  nombre            String
  cargo             String
  email             String
  telefono          String
  rut               Int
  digitoVerificador String
  empresaId         String
  tipoContactoId    Int

  tipoContacto      TipoContacto @relation(fields: [tipoContactoId], references: [id])
  empresa           Empresa      @relation(fields: [empresaId], references: [id])

  @@map("Contacto")
}