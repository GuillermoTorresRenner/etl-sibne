//REVISADO
model Encuesta {
  id                    Int                @id @default(autoincrement())
  nombre                String
  anio                  Int
  fechaCreacion         DateTime
  fechaInicio           DateTime?
  fechaTermino          DateTime?
  margenError           Int
  valorDolar            Float?
  activo                Boolean
  archivoAdjuntoid      Int?

  archivoAdjuntoEntidad ArchivoAdjunto?    @relation("ArchivoAdjuntoEntidad", fields: [archivoAdjuntoid], references: [id])
  encuestaEmpresa       EncuestaEmpresa[]
  encuestaPlanta        EncuestaPlanta[]
  intensidadEnergEncuestaEmpresas IntensidadEnergEncuestaEmpresa[]
  reportes                Reporte[]
  transacciones           Transaccion[]

  @@map("Encuesta")
}