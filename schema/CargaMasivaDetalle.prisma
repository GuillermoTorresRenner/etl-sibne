model CargaMasivaDetalle {
  id                  Int                @id @default(autoincrement())
  cargaMasivaArchivoId Int
  nombreHoja          String
  fechaIngreso        DateTime?
  estado              String
  registrosCargado    Int
  registrosConError   Int
  totalRegistros      Int

  cargaMasivaArchivo  CargaMasivaArchivo @relation(fields: [cargaMasivaArchivoId], references: [id])
  cargaMasivaErrors   CargaMasivaError[]

  @@map("CargaMasivaDetalle")
}